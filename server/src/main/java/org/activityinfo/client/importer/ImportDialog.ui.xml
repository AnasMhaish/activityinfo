<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
 	.popup {
		background-color: white;
	}
  </ui:style>
  <g:DockLayoutPanel ui:field="dockLayoutPanel" styleName="{style.popup}">
    <g:center>
      <g:SimpleLayoutPanel ui:field="pagePanel"/>
    </g:center>
    <g:south size="30">
      <g:FlowPanel>
        <g:Button ui:field="cancelButton" text="Cancel"/> 
        
        <g:Button ui:field="prevButton" text="« Previous"/>
        <g:Button ui:field="nextButton" text="Next »"/>
      </g:FlowPanel>
    </g:south>
  </g:DockLayoutPanel>
</ui:UiBinder> 