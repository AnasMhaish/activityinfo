<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:coord="urn:import:org.activityinfo.ui.full.client.widget.coord">

  <g:HTMLPanel addStyleNames="bs">
    <g:HorizontalPanel spacing="3">
      <coord:GwtCoordinateField ui:field="latitude" axis="LATITUDE"/>
      <coord:GwtCoordinateField ui:field="longitude" axis="LONGITUDE"/>
    </g:HorizontalPanel>
  </g:HTMLPanel>
</ui:UiBinder>