<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <g:HTMLPanel addStyleNames="bs">
    <div class="container">
      <div class="has-error">
        <div class="control-label" ui:field="errorContainer"/>
      </div>
      <g:FlowPanel ui:field="contentPanel" width="100%">
      </g:FlowPanel>
      <div class="pull-right">
        <g:Button addStyleNames="btn btn-primary" ui:field="saveButton" text="Save"/>
        <!--&nbsp;-->
        <g:Button addStyleNames="btn btn-default" ui:field="resetButton" text="Reset"/>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>