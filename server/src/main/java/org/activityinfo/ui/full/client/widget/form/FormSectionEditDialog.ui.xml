<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <g:HTMLPanel addStyleNames="bs">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <g:Button addStyleNames="close" ui:field="closeButton" text="Ã—"/>
          <h4 class="modal-title">Edit section</h4>
        </div>
        <div class="modal-body">
          Section label:
          <g:TextBox/>
        </div>
        <div class="modal-footer">
          <g:Button addStyleNames="btn btn-primary" text="OK"
                    enabled="false" ui:field="continueButton"/>
          <g:Button addStyleNames="btn btn-default" text="Cancel"
                    ui:field="cancelButton"/>
        </div>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>