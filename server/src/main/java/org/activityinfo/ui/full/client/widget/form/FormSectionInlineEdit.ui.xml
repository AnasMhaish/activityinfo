<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:with field='res' type='org.activityinfo.ui.full.client.i18n.UIConstants'/>
  <ui:with field='style' type='org.activityinfo.ui.full.client.style.TransitionBundle'/>

  <g:HTMLPanel addStyleNames="bs">
    <g:HTMLPanel>
      <!--<g:Button addStyleNames="close" ui:field="closeButton" text="Ã—"/>-->
      <!--<h4 class="modal-title" ui:field="title"/>-->
      <!--<ui:text from='{res.sectionLabel}'/>-->
      <g:TextBox addStyleNames="form-control" width="100%" ui:field="sectionLabel" styleName="ai"/>
      <div style="margin-top: 5px; margin-bottom: 5px;">
        <g:Button addStyleNames="btn btn-primary" text="{res.ok}" ui:field="okButton"/>
        <g:Button addStyleNames="btn btn-default" text="{res.cancel}" ui:field="cancelButton"/>
      </div>
    </g:HTMLPanel>
  </g:HTMLPanel>
</ui:UiBinder>