<module rename-to="AI">

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit additional dependencies  -->
  <inherits name='com.google.gwt.inject.Inject'/>
  <inherits name="com.google.gwt.i18n.I18N"/>
  <inherits name="com.google.gson.Gson"/>

  <inherits name="org.discotools.gwt.leaflet.Core"/>

  <!--<inherits name="com.extjs.gxt.charts.Chart"/>-->
  <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>

  <inherits name="com.google.common.collect.Collect"/>

  <inherits name="com.bedatadriven.rebar.style.Style"/>

  <inherits name="org.activityinfo.i18n.I18n"/>
  <inherits name="org.activityinfo.model.Model"/>
  <inherits name="org.activityinfo.service.Service"/>
  <inherits name="org.activityinfo.ui.store.remote.RemoteStore"/>

  <inherits name="org.activityinfo.legacy.Legacy"/>
  <inherits name="org.activityinfo.ui.widget.barcode.Barcode"/>

  <set-property name="gwt.logging.popupHandler" value="DISABLED"/>


  <!-- Include generated sources -->
  <source path='client'/>
  <source path="shared"/>

  <public path="assets"/>

  <entry-point class="org.activityinfo.ui.client.ActivityInfoEntryPoint3"/>

</module>
