<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:a='urn:import:org.activityinfo.ui.client.widget'>

    <ui:with field="icons" type="org.activityinfo.ui.client.style.Icons"/>

    <g:HTMLPanel>


        <h1><span ui:field="pageIcon"/><span ui:field="nameElement"/></h1>

        <div ui:field="breadCrumbElement"/>

        <ul class="nav nav-tabs">
          <li><a href="#"><span class="{icons.overview}"/> Overview</a></li>
          <li class="active"><a href="#"><span class="{icons.table}"/> Table</a></li>
          <li><a href="#"><span class="{icons.map}"/> Map</a></li>
        </ul>

        <a:LoadingPanel ui:field="contentPanel"/>


    </g:HTMLPanel>
</ui:UiBinder>