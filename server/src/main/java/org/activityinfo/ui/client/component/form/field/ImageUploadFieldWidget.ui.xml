<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
        >

  <ui:with field="i18n" type="org.activityinfo.i18n.shared.UiConstants"/>

  <g:FormPanel encoding="multipart/form-data" method="post">
    <g:HTMLPanel>
      <div class="form-group">
        <g:FileUpload ui:field="fileUpload" name="file"/>
      </div>
      <div class="form-group">
        <g:Button addStyleNames="btn btn-default btn-xs" ui:field="uploadButton">
          <ui:text from='{i18n.upload}'/>
        </g:Button>
      </div>
    </g:HTMLPanel>
  </g:FormPanel>
</ui:UiBinder>