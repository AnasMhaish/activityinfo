<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>

  <ui:with field='name' type='java.lang.String' />
  <ui:with field='link' type='com.google.gwt.safehtml.shared.SafeUri' />
  <ui:with field='b' type='com.bedatadriven.rebar.bootstrap.client.Bootstrap'/>
  
  <div class="{b.well} {b.media}">
    <a class="{b.pullLeft}" href="{link}">
      <img class="media-object" src="//placehold.it/64x64" />
    </a>
    <div class="{b.mediaBody}">
      <h4 class="{b.mediaHeading}"><ui:text from="{name}"/> <small>DR Congo</small></h4>
      <p>Monitoring database for Syrian Refugee Response</p>
    </div>
  </div>
</ui:UiBinder>
