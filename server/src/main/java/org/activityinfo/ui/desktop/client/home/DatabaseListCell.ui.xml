<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

  <ui:with field='name' type='java.lang.String' />
  <ui:with field='link' type='com.google.gwt.safehtml.shared.SafeUri' />

  <div class="well media">
    <a class="pull-left" href="{link}">
      <img class="media-object" src="http://placehold.it/64x64" />
    </a>
    <div class="media-body">
      <h4 class="media-heading"><ui:text from="{name}"/> <small>DR Congo</small></h4>
      <p>Monitoring database for Syrian Refugee Response</p>
    </div>
  </div>
</ui:UiBinder>
