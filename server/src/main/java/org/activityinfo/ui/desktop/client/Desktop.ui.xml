<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field='b' type='com.bedatadriven.rebar.bootstrap.client.Bootstrap'/>
  <g:HTMLPanel>
    <nav class="{b.navbar} {b.navbarInverse} {b.navbarFixedTop}" role="navigation">
      <div class="{b.navbarHeader}">
        <a class="{b.navbarBrand}" href="#">ActivityInfo</a>
      </div>
    </nav>
    <div class="{b.container}">
      <g:SimplePanel ui:field="main"/>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>