<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >

  <ui:with field="i18n" type="org.activityinfo.i18n.shared.UiConstants"/>
  <ui:with field="style" type="org.activityinfo.ui.client.component.formdesigner.FormDesignerStyles"/>

  <g:HTMLPanel width="100%">
    <div class="row form-group">
      <label class="control-label col-xs-4" ui:field="label">
        <ui:text from='{i18n.cardinality}'/>
      </label>
      <div class="col-xs-8">
        <g:ListBox ui:field="cardinality"/>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>